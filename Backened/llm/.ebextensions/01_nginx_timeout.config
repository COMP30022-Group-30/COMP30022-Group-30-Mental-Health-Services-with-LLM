files:
  "/etc/nginx/conf.d/proxy_timeout.conf":
    mode: "000644"
    owner: root
    group: root
    content: |
      proxy_connect_timeout 60s;
      proxy_send_timeout 120s;
      proxy_read_timeout 120s;
      send_timeout 120s;

container_commands:
  01_reload_nginx:
    command: "service nginx reload || true"
